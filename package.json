    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "eslint": "^3.15.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "babel-runtime": "6.22.0",
    "compressible": "^2.0.10",
    "mime-types": "^2.1.15",
    "think_lib": "^1.x.x"
